# prompt prompt
#     match /[\/a-zA-Z0-9._-]+ ?(?:\(config[^)]*\))? ?[#>] ?$/

prompt basic
    match /> ?$/

prompt privileged
    match /# ?$/

prompt configure
    match /\(config[^)]*\)# ?$/

prompt user
    match /[Uu]sername/

prompt pass
    match /[Pp]assword: ?$/

# prompt userpass
#     match /(?:[Uu]sername|[Pp]assword): ?$/

macro begin_privileged
    send enable

# macro begin_privileged_with_user
#     send login

macro end_privileged
    send disable

macro begin_configure
    send configure terminal

macro end_configure
    send exit

macro disconnect
    send exit

